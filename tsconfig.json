{
  "extends": "./tsconfig.build.json",
  "compilerOptions": {
    "baseUrl": "./packages",
    "paths": {
      "txwrapper-acala": [
        "txwrapper-acala/src"
      ],
      "txwrapper-acala/*": [
        "txwrapper-acala/src/*"
      ],
      "@axia-core/txwrapper-core": [
        "txwrapper-core/src"
      ],
      "@axia-core/txwrapper-core/*": [
        "txwrapper-core/src/*"
      ],
      "@axia-core/txwrapper-examples": [
        "txwrapper-examples/src"
      ],
      "@axia-core/txwrapper-examples/*": [
        "txwrapper-examples/src/*"
      ],
      "@axia-core/txwrapper-orml": [
        "txwrapper-orml/src"
      ],
      "@axia-core/txwrapper-orml/*": [
        "txwrapper-orml/src/*"
      ],
      "@axia-core/txwrapper-axia": [
        "txwrapper-axia/src"
      ],
      "@axia-core/txwrapper-axia/*": [
        "txwrapper-axia/src/*"
      ],
      "@axia-core/txwrapper-registry": [
        "txwrapper-registry/src"
      ],
      "@axia-core/txwrapper-registry/*": [
        "txwrapper-registry/src/*"
      ],
      "@axia-core/txwrapper-axia-core": [
        "txwrapper-axia-core/src"
      ],
      "@axia-core/txwrapper-axia-core/*": [
        "txwrapper-axia-core/src/*"
      ]
    }
  }
}
